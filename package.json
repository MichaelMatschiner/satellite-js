{
  "name": "satellite.js",
  "version": "1.4.0",
  "description": "SGP4/SDP4 calculation library",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "transpile": "babel src -d lib",
    "dist": "webpack",
    "dist:development": "cross-env NODE_ENV=development npm run dist",
    "dist:production": "cross-env NODE_ENV=production npm run dist",
    "build": "npm run transpile && npm run dist:development && npm run dist:production",
    "rebuild": "npm run clean && npm run build",
    "lint": "eslint ./src",
    "lint:test": "eslint -c .eslintrc.test.json ./test",
    "test": "mocha --recursive --require babel-register test",
    "prepublish": "npm run lint && npm run lint:test && npm test && npm run rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shashwatak/satellite-js"
  },
  "keywords": [
    "sgp4",
    "satellite"
  ],
  "author": "Shashwat Kandadai <shashwatak@gmail.com> (https://github.com/shashwatak)",
  "contributors": [
    "Dmitriy Pushkov <ezze@ezze.org> (http://www.ezze.org)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shashwatak/satellite-js/issues"
  },
  "dependencies": {
    "object-assign": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "webpack": "^2.3.3"
  },
  "homepage": "https://github.com/shashwatak/satellite-js",
  "directories": {
    "test": "test"
  }
}
